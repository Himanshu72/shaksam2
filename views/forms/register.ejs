        <body >
            <div class="container">
        <!-- Phone number -->
        <form action="/customer/reg" method="POST"  >
            
            <p class="h4 mb-4 text-center">Sign up</p>
    
            <div class="form-row mb-4" style="margin-top:4%;" >
                <div class="col-12">
                    <!-- First name -->
                    <input type="text" id="defaultRegisterFormFirstName"   name="fname" class="form-control" placeholder="First Name">
                </div>
                
                <br>
                <div class="col-12">
                    <!-- Last name -->
                    <input type="text" id="defaultRegisterFormLastName" name="lname" class="form-control" placeholder="Last Name">
                </div>
            </div>
        
            <!-- E-mail -->
            <div class="row">
                <div class="col-12">
                <input type="email" id="defaultRegisterFormEmail" name="email" class="form-control" placeholder="E-mail">
        
        </div>
            <!-- Password -->
            
            <div class="col-12">
            <input type="text" id="defaultRegisterFormPassword"  name="age" class="form-control" placeholder="Age" aria-describedby="defaultRegisterFormPasswordHelpBlock">
          </div>
            </div>
            
            <div class="row">
            <div class="col-12" >
        <br>
                <input type="text" id="defaultRegisterPhonePassword" class="form-control" name="ph"  placeholder="Phone Number" aria-describedby="defaultRegisterFormPhoneHelpBlock">
                <br>
                <input type="text" id="defaultRegisterPhonePassword" class="form-control" name="gender"  placeholder="Gender" aria-describedby="defaultRegisterFormPhoneHelpBlock">
               
                <br>
                <input type="text" id="defaultRegisterPhonePassword" class="form-control" name="collage" placeholder="College or University Name" aria-describedby="defaultRegisterFormPhoneHelpBlock">
                <br>
                <input type="text" id="defaultRegisterPhonePassword" class="form-control" name="add"   placeholder="Your Address" aria-describedby="defaultRegisterFormPhoneHelpBlock">
                
                <br>
                <input type="text" id="defaultRegisterPhonePassword" class="form-control" name="cur"   placeholder="Current Course Pursuing " aria-describedby="defaultRegisterFormPhoneHelpBlock">
                
                <br>
                <input type="password" id="defaultRegisterPhonePassword" class="form-control" placeholder="Password" name="pass" aria-describedby="defaultRegisterFormPhoneHelpBlock">
                <br>
                <input type="password" id="defaultRegisterPhonePassword" class="form-control" name="cpass"  placeholder="Confrim Password" aria-describedby="defaultRegisterFormPhoneHelpBlock">
                <br>
                <textarea class="form-control"  name="skill"  placeholder="Skills you can perform"  ></textarea>
                <br>
                <textarea class="form-control" name="others"   placeholder="Anything else you would like us to know about you"  ></textarea>     
            </div>
    </div>
    
        <!-- Newsletter -->
       
        <!-- Sign up button -->
        <button class="btn btn-info my-4 btn-block" type="submit">register</button>
    
